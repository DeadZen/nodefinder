{deps,
 [{erlcloud, ".*",
   {git, "https://github.com/gleber/erlcloud.git",
    "596cbcde53b84f78d76e7427573833daa9201c79"}}]}.

{erl_opts, [warnings_as_errors,
            strict_validation,
            warn_bif_clash,
            warn_deprecated_function,
            warn_export_all,
            warn_export_vars,
            warn_exported_vars,
            warn_obsolete_guard,
            warn_shadow_vars,
            warn_unused_import,
            warn_unused_function,
            warn_unused_record,
            warn_unused_vars]}.
{cover_enabled, true}.
{cover_print_enabled, true}.
